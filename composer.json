{
  "name": "ixspx/module-generator",
  "description": "Generate module structure in Laravel",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "module",
    "module-generator",
    "mod"
  ],
  "authors": [
    {
      "name": "saul-paulus",
      "email": "saulpaulus17@icloud.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1 || ^8.3",
    "illuminate/console": "^10.0 || ^11.0 || ^12.0",
    "illuminate/filesystem": "^10.0 || ^11.0 || ^12.0",
    "illuminate/support": "^10.0 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "Ixspx\\ModuleGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ixspx\\ModuleGenerator\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
